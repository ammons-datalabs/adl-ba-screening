    "BccParcelMetricsPlanFile": "bcc/plan-metrics.ndjson",
    "AddressesFile": "bcc/addresses.ndjson"
  },
  "Geocoding": {
    "Provider": "AzureMaps"
  },
  "AzureMaps": {
    "SubscriptionKey": ""
  }
}
